<%= form_for @television, url: user_televisions_path do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :brand %>
  <%= f.text_field :brand %>
  <%= f.label :cable_company %>
  <%= f.text_field :cable_company %>
  <%= f.submit "Create", class: "btn btn-primary" %>
<% end %>

