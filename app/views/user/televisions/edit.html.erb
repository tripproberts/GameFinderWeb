<%= form_for @television, url: user_television_path, method: :patch do |f| %>

  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :brand %>
    <%= f.text_field :brand, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :cable_company %>
    <%= f.text_field :cable_company, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.submit "Update", class: "btn btn-primary" %>
  </div>
<% end %>

